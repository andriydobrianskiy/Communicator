<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.mainPage.MainPageController">
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <BorderPane BorderPane.alignment="CENTER">
               <center>
                  <JFXTabPane fx:id="TabPane" styleClass="MainPage.css" tabClosingPolicy="UNAVAILABLE">
                     <tabs>
                        <Tab fx:id="tabDictionary" text="Не виконані">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0">
                                 <children>
                                    <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <center>
                                          <fx:include fx:id="notFulledView" source="NotFulledView.fxml" />
                                       </center>
                                    </BorderPane>
                                 </children>
                              </AnchorPane>
                          </content>
                        </Tab>
                        <Tab fx:id="tabInProcessing" text="В обробці">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <center>
                                          <fx:include fx:id="InProcessingView" source="../../mainPage/InProcessing/InProcessingView.fxml" />
                                       </center>
                                    </BorderPane>
                                 </children>
                              </AnchorPane>
                          </content>
                        </Tab>
                        <Tab fx:id="tabInTract" text="В тракті ">
                          <content>
                            <AnchorPane>
                                 <children>
                                    <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <center>
                                          <fx:include fx:id="inTractView" source="InTractView.fxml" />
                                       </center>
                                    </BorderPane>
                                 </children>
                              </AnchorPane>
                          </content>
                        </Tab>
                        <Tab fx:id="tabArchive" text="Архівні">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <center>
                                          <fx:include fx:id="archiveFilesView" source="ArchiveFilesView.fxml" />
                                       </center>
                                    </BorderPane>
                                 </children>
                              </AnchorPane>
                          </content>
                        </Tab>
                        <Tab fx:id="tabAll" text="Всі">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0">
                                 <children>
                                    <BorderPane>
                                       <center>
                                          <fx:include fx:id="allView" source="AllView.fxml" />
                                       </center>
                                    </BorderPane>
                                 </children></AnchorPane>
                          </content>
                        </Tab>
                        <Tab fx:id="tabStatistic" text="Статистика">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                          </content>
                        </Tab>
                        <Tab fx:id="tabStructure" text="Структура відділу закупів">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <BorderPane>
                                       <center>
                                          <fx:include fx:id="structureView" source="StructureView.fxml" />
                                       </center>
                                    </BorderPane>
                                 </children></AnchorPane>
                              
                          </content>
                        </Tab>
                     </tabs>
                  </JFXTabPane>
               </center>
            </BorderPane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
