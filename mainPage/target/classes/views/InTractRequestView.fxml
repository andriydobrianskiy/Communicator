<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mainPage.InTract.InTractRequest.InTractRequestController">
   <center>
      <TableView fx:id="tableViewRequest" editable="true">
         <columns>
            <TableColumn fx:id="columnIndex" prefWidth="75.0" text="Індекс" />
            <TableColumn fx:id="columnSkrut" prefWidth="75.0" text="Скорочення " />
            <TableColumn fx:id="columnNewDescription" prefWidth="75.0" text="Назва/Опис" />
            <TableColumn fx:id="columnOfferingName" prefWidth="75.0" text="Продукт" />
            <TableColumn fx:id="columnQuantity" prefWidth="75.0" sortType="DESCENDING" text="Кількість" />
            <TableColumn fx:id="columnDefaultOfferingCode" prefWidth="75.0" sortable="false" text="Код товару" />
            <TableColumn fx:id="columnNewOfferingCode" prefWidth="75.0" text="Код товару (новий)" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
   </center>
</BorderPane>
